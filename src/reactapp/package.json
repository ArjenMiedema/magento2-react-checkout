{
  "name": "vite-hyva-checkout",
  "version": "2.0-develop",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "build": "vite build",
    "serve": "vite preview",
    "dev:twc": "tailwindcss --postcss -i tailwind/tailwind.css -o src/index.css --watch",
    "build:twc": "postcss -i tailwind/tailwind.css -o tailwind/build.css --minify",
    "dev:vite": "vite",
    "preinstall": "node scripts/configurePaymentMethods && node scripts/configureThemes"
  },
  "config": {
    "themeRepositories": "Add your themes here",
    "paymentMethodsRepo": "Add your payment methods here"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.5",
    "@vitejs/plugin-react": "^1.0.0",
    "formik": "^2.2.9",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "preact": "^10.6.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "vite": "^2.6.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@tailwindcss/forms": "^0.3.4",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.4.1",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.2",
    "postcss-import": "^14.0.2",
    "prettier": "^2.5.1",
    "tailwindcss": "^2.2.19"
  }
}
